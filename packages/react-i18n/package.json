{
  "name": "@shopify/react-i18n",
  "version": "7.8.0",
  "license": "MIT",
  "description": "i18n utilities for React handling translations, formatting, and more",
  "main": "index.js",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "babel": [
        "./build/ts/babel-plugin/index.d.ts"
      ],
      "generate-dictionaries": [
        "./build/ts/babel-plugin/generate-dictionaries.d.ts"
      ],
      "generate-index": [
        "./build/ts/babel-plugin/generate-index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/react-i18n"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/react-i18n/README.md",
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/types": ">=7.0.0",
    "@types/babel__core": "^7.0.4",
    "@types/babel__template": "^7.0.0",
    "@types/babel__traverse": "^7.0.0",
    "@types/glob": "^7.1.1",
    "intl-pluralrules": "^0.2.1"
  },
  "dependencies": {
    "@shopify/dates": "^2.0.3",
    "@shopify/function-enhancers": "^3.0.1",
    "@shopify/i18n": "^2.0.1",
    "@shopify/react-effect": "^5.0.3",
    "@shopify/react-hooks": "^3.0.5",
    "@shopify/useful-types": "^5.1.2",
    "@types/hoist-non-react-statics": "^3.0.1",
    "change-case": "^4.1.2",
    "glob": "^7.1.4",
    "hoist-non-react-statics": "^3.0.1",
    "lodash.clonedeep": "^4.0.0",
    "lodash.merge": "^4.0.0",
    "string-hash": "^1.1.3",
    "strip-json-comments": "^3.1.1"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0"
  },
  "optionalDependencies": {
    "@babel/template": "^7.0.0",
    "@babel/traverse": "^7.0.0",
    "fs-extra": "^9.1.0"
  },
  "files": [
    "build/",
    "!build/*.tsbuildinfo",
    "!build/ts/**/tests/",
    "index.js",
    "index.mjs",
    "index.esnext",
    "babel.js",
    "babel.mjs",
    "babel.esnext",
    "generate-dictionaries.js",
    "generate-dictionaries.mjs",
    "generate-dictionaries.esnext",
    "generate-index.js",
    "generate-index.mjs",
    "generate-index.esnext"
  ],
  "module": "index.mjs",
  "esnext": "index.esnext",
  "exports": {
    "./babel": {
      "types": "./build/ts/babel-plugin/index.d.ts",
      "default": "./babel.js"
    },
    "./generate-dictionaries": {
      "types": "./build/ts/babel-plugin/generate-dictionaries.d.ts",
      "esnext": "./generate-dictionaries.esnext",
      "import": "./generate-dictionaries.mjs",
      "require": "./generate-dictionaries.js"
    },
    "./generate-index": {
      "types": "./build/ts/babel-plugin/generate-index.d.ts",
      "esnext": "./generate-index.esnext",
      "import": "./generate-index.mjs",
      "require": "./generate-index.js"
    },
    ".": {
      "types": "./build/ts/index.d.ts",
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  }
}
