{
  "name": "@shopify/react-import-remote",
  "version": "2.1.4",
  "license": "MIT",
  "description": "Asynchronous script loading for React",
  "main": "index.js",
  "types": "./build/ts/index.d.ts",
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/react-import-remote"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/react-import-remote/README.md",
  "engines": {
    "node": ">=12.14.0"
  },
  "dependencies": {
    "@shopify/async": "^3.0.4",
    "@shopify/react-hooks": "^2.1.3",
    "@shopify/react-html": "^11.1.4",
    "@shopify/react-intersection-observer": "^3.1.3",
    "@shopify/useful-types": "^3.0.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <18.0.0"
  },
  "sideEffects": false,
  "files": [
    "build/",
    "!build/*.tsbuildinfo",
    "!build/ts/**/tests/",
    "index.js",
    "index.mjs",
    "index.esnext"
  ],
  "module": "index.mjs",
  "esnext": "index.esnext",
  "exports": {
    "./": "./",
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "esnext": "./index.esnext"
    }
  }
}
