  "engines": {
    "node": ">=18.12.0"
  },
  "dependencies": {
    "@shopify/react-graphql": "^10.3.2",
    "@shopify/react-hooks": "^4.1.1",
    "@shopify/react-html": "^13.2.2",
    "@shopify/react-network": "^5.2.2"
  },
  "peerDependencies": {
    "@apollo/client": ">=3.5.8 || <4.0.0",
