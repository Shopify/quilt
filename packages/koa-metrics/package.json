    "directory": "packages/koa-metrics"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/koa-metrics/README.md",
  "engines": {
    "node": ">=18.12.0"
  },
  "dependencies": {
    "@shopify/statsd": "^4.5.0"
  },
  "devDependencies": {
    "@shopify/jest-koa-mocks": "^5.2.0",
