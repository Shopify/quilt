{
  "name": "@shopify/web-worker",
  "version": "3.0.2",
  "license": "MIT",
  "description": "Tools for making web workers fun to use",
  "main": "index.js",
  "module": "index.mjs",
  "esnext": "index.esnext",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "babel": [
        "./build/ts/babel-plugin.d.ts"
      ],
      "webpack": [
        "./build/ts/webpack-parts/index.d.ts"
      ],
      "worker": [
        "./build/ts/worker.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./": "./",
    "./babel": {
      "esnext": "./babel.esnext",
      "import": "./babel.mjs",
      "require": "./babel.js"
    },
    "./webpack": {
      "esnext": "./webpack.esnext",
      "import": "./webpack.mjs",
      "require": "./webpack.js"
    },
    "./worker": {
      "esnext": "./worker.esnext",
      "import": "./worker.mjs",
      "require": "./worker.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/web-worker"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/web-worker/README.md",
  "engines": {
    "node": ">=12.14.0"
  },
  "files": [
    "build/",
    "!build/*.tsbuildinfo",
    "!build/ts/**/tests/",
    "index.js",
    "index.mjs",
    "index.esnext",
    "babel.js",
    "babel.mjs",
    "babel.esnext",
    "webpack.js",
    "webpack.mjs",
    "webpack.esnext",
    "worker.js",
    "worker.mjs",
    "worker.esnext"
  ],
  "dependencies": {
    "@remote-ui/rpc": "^1.2.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/get-port": "^4.2.0",
    "@types/koa": "^2.11.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/loader-utils": "^2.0.0",
    "@types/webpack": ">=4 <5",
    "@types/webpack-virtual-modules": "^0.1.0",
    "babel-loader": "^8.2.0",
    "fs-extra": "^9.0.0",
    "get-port": "^5.1.0",
    "koa": "^2.11.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "loader-utils": "^2.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "webpack": ">=4 <5",
    "webpack-virtual-modules": "^0.4.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "@types/webpack": ">=4 <5",
    "loader-utils": "^2.0.0",
    "webpack": ">=4 <5",
    "webpack-virtual-modules": "^0.4.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@types/webpack": {
      "optional": true
    },
    "loader-utils": {
      "optional": true
    },
    "webpack-virtual-modules": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
