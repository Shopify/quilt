{
  "name": "@shopify/react-form",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Manage React forms tersely and safely-typed with no magic using React hooks",
  "main": "index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/react-form"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/react-form/README.md",
  "engines": {
    "node": ">=12.14.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <17.0.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "faker": "^4.1.0"
  },
  "files": [
    "build/*",
    "!*.tsbuildinfo",
    "index.js",
    "index.mjs",
    "index.esnext",
    "index.d.ts"
  ],
  "dependencies": {
    "@shopify/predicates": "^2.0.0",
    "@shopify/react-hooks": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
