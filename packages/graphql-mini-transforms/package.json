{
  "name": "graphql-mini-transforms",
  "version": "5.3.5",
  "license": "MIT",
  "description": "Transformers for importing .graphql files in various build tools.",
  "main": "index.js",
  "types": "./build/ts/index.d.ts",
  "typesVersions": {
    "*": {
      "jest": [
        "./build/ts/jest.d.ts"
      ],
      "jest-simple": [
        "./build/ts/jest-simple.d.ts"
      ],
      "rollup": [
        "./build/ts/rollup.d.ts"
      ],
      "webpack": [
        "./build/ts/webpack-loader.d.ts"
      ],
      "webpack-loader": [
        "./build/ts/webpack-loader.d.ts"
      ],
      "vite": [
        "./build/ts/vite.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "@shopify:registry": "https://registry.npmjs.org"
  },
  "author": "Shopify Inc.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shopify/quilt.git",
    "directory": "packages/graphql-mini-transforms"
  },
  "bugs": {
    "url": "https://github.com/Shopify/quilt/issues"
  },
  "homepage": "https://github.com/Shopify/quilt/blob/main/packages/graphql-mini-transforms/README.md",
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "dependencies": {
    "@jest/transform": ">= 27 <29",
    "fs-extra": "^9.1.0",
    "graphql": ">=14.5.0 <17.0.0",
    "graphql-typed": "^2.0.3"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "common-tags": "^1.8.0",
    "webpack": "^5.38.0"
  },
  "peerDependencies": {
    "rollup": "^3.0.0 || ^4.0.0",
    "webpack": "^5.38.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "files": [
    "build/",
    "!build/*.tsbuildinfo",
    "!build/ts/**/tests/",
    "index.js",
    "index.mjs",
    "index.esnext",
    "jest.js",
    "jest.mjs",
    "jest.esnext",
    "jest-simple.js",
    "jest-simple.mjs",
    "jest-simple.esnext",
    "rollup.js",
    "rollup.mjs",
    "rollup.esnext",
    "webpack-loader.js",
    "webpack-loader.mjs",
    "webpack-loader.esnext",
    "vite.js",
    "vite.mjs",
    "vite.esnext"
  ],
  "module": "index.mjs",
  "esnext": "index.esnext",
  "exports": {
    "./jest": {
      "types": "./build/ts/jest.d.ts",
      "default": "./jest.js"
    },
    "./jest-simple": {
      "types": "./build/ts/jest-simple.d.ts",
      "default": "./jest-simple.js"
    },
    "./rollup": {
      "types": "./build/ts/rollup.d.ts",
      "esnext": "./rollup.esnext",
      "import": "./rollup.mjs",
      "require": "./rollup.js"
    },
    "./webpack-loader": {
      "types": "./build/ts/webpack-loader.d.ts",
      "default": "./webpack-loader.js"
    },
    "./webpack": {
      "types": "./build/ts/webpack-loader.d.ts",
      "default": "./webpack-loader.js"
    },
    "./vite": {
      "types": "./build/ts/vite.d.ts",
      "esnext": "./vite.esnext",
      "import": "./vite.mjs",
      "require": "./vite.js"
    },
    ".": {
      "types": "./build/ts/index.d.ts",
      "esnext": "./index.esnext",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "tags": [
    "graphql",
    "apollo",
    "loader",
    "plugin",
    "rollup",
    "webpack",
    "webpack-loader",
    "vite",
    "jest"
  ]
}
